{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/(api|server)/?(.*)", "status": 404, "continue": false },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
